digraph figsync
{
  nodesep = 0;

  nodeinp [label = "Input", shape = plaintext];
  nodeoutp [label = "Output", shape = plaintext];
  nodemem [shape=record, label = "Memory", width = 2, height=.5];
  node [shape=record,width=3,height=1];
  nodeclw [label = "{ <f0> | <f1> | <f2>}" , style="invis", fixedsize=true, width=0.001, sep=0.001 ];
  nodecl [label = "CL", sep=0.001 ];
  nodecle [label = "{ <f0> | <f1> | <f2>}" , style="invis", fixedsize=true, width=0.001, sep=0.001 ];
  nodecl->nodecle [style=invis];
  nodeclw->nodecl [style=invis];
/*
  nodecl [shape=plaintext, label=<
    <TABLE BORDER="1" CELLBORDER="0" CELLSPACING="20" CELLPADDING="0">
      <TR><TD PORT="f1">CL</TD></TR>
      <TR><TD PORT="f2" COLSPAN="20"></TD></TR>
    </TABLE>>];
*/

  {rank=same nodeinp nodeclw nodecl nodecle nodeoutp};
  nodeinp:e->nodeclw:f1:w [minlen = 4];
  nodecle:f1:e->nodeoutp:w [minlen = 4];
  nodecle:f2:e -> nodemem:e [minlen = 4];
  nodemem:w -> nodeclw:f2:w [minlen = 4];
}

